 <div class="ky-co">
            <div class="bg-2 cebox" style="padding: 25px; border-radius: 11px;">
                <div class="h"><%= copyTrade ? 'Edit' : 'Add' %> Copy Trader</div>
                
                <% if (messages.error) { %>
                    <div class="alert alert-danger mt-3"><%= messages.error %></div>
                <% } %>
                <% if (messages.success) { %>
                    <div class="alert alert-success mt-3"><%= messages.success %></div>
                <% } %>

                <form action="<%= copyTrade ? '/edit-copytrader/' + copyTrade._id : '/add-copytrader' %>" method="POST" enctype="multipart/form-data" id="copyTradeForm">
                    <div class="form-group">
                        <label for="traderName" class="text-white">Trader Name</label>
                        <input type="text" class="form-control" id="traderName" name="traderName" value="<%= copyTrade ? copyTrade.traderName : '' %>" required>
                    </div>
                    <div class="form-group">
                        <label for="traderImage" class="text-white">Trader Image</label>
                        <input type="file" class="form-control-file" id="traderImage" name="traderImage" accept="image/jpeg,image/jpg,image/png">
                        <% if (copyTrade && copyTrade.traderImage) { %>
                            <img src="<%= copyTrade.traderImage %>" class="preview-img" alt="Current Image">
                        <% } %>
                        <img id="imagePreview" class="preview-img" style="display: none;">
                    </div>
                    <div class="form-group">
                        <label for="profitShare" class="text-white">Profit Share (%)</label>
                        <input type="number" class="form-control" id="profitShare" name="profitShare" min="0" max="100" step="0.01" value="<%= copyTrade ? copyTrade.profitShare : '' %>" required>
                    </div>
                    <div class="form-group">
                        <label for="winRate" class="text-white">Win Rate (%)</label>
                        <input type="number" class="form-control" id="winRate" name="winRate" min="0" max="100" step="0.01" value="<%= copyTrade ? copyTrade.winRate : '' %>" required>
                    </div>
                    <button type="submit" class="btn bg-4 text-white">Save Copy Trader</button>
                    <a href="/all-copytrades" class="btn btn-secondary ml-2">Cancel</a>
                </form>
            </div>
        </div>

