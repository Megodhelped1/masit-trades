        $(document).ready(function() {
            // Check if user is authenticated
            const userId = '<%= user ? user._id : '' %>';
            if (!userId) {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'User not authenticated. Please log in.',
                    timer: 2000
                }).then(() => {
                    window.location = '/signin';
                });
            }
        });
