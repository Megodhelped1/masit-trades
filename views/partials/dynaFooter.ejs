     <script>
        // Image preview
        document.getElementById('traderImage').addEventListener('change', function(e) {
            const file = e.target.files[0];
            if (file) {
                if (!/image\/(jpeg|jpg|png)/.test(file.type)) {
                    alert('Please upload a valid image (jpg, jpeg, or png).');
                    this.value = '';
                    return;
                }
                if (file.size > 5 * 1024 * 1024) {
                    alert('File size must be less than 5MB.');
                    this.value = '';
                    return;
                }
                const reader = new FileReader();
                reader.onload = function(e) {
                    const preview = document.getElementById('imagePreview');
                    preview.src = e.target.result;
                    preview.style.display = 'block';
                };
                reader.readAsDataURL(file);
            }
        });

        // Form validation
        document.getElementById('copyTradeForm').addEventListener('submit', function(e) {
            const traderName = document.getElementById('traderName').value;
            const profitShare = parseFloat(document.getElementById('profitShare').value);
            const winRate = parseFloat(document.getElementById('winRate').value);

            if (!traderName || traderName.trim() === '') {
                e.preventDefault();
                alert('Trader name is required.');
                return;
            }
            if (isNaN(profitShare) || profitShare < 0 || profitShare > 100) {
                e.preventDefault();
                alert('Profit share must be between 0 and 100.');
                return;
            }
            if (isNaN(winRate) || winRate < 0 || winRate > 100) {
                e.preventDefault();
                alert('Win rate must be between 0 and 100.');
                return;
            }
        });
    </script>


