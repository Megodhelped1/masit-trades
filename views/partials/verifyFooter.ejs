        <script>
    $(document).ready(function() {
        // Wizard navigation
        var current = 1;
        var steps = $("fieldset").length;

        $(".next").click(function() {
            var currentFieldset = $(this).closest("fieldset");
            // Validate fields in the current step
            if (validateStep(currentFieldset)) {
                currentFieldset.hide();
                currentFieldset.next().show();
                $("#top-tab-list li").eq(current - 1).removeClass("active");
                $("#top-tab-list li").eq(current).addClass("active");
                current++;
            }
        });

        $(".previous").click(function() {
            current--;
            $(this).closest("fieldset").hide();
            $(this).closest("fieldset").prev().show();
            $("#top-tab-list li").eq(current).removeClass("active");
            $("#top-tab-list li").eq(current - 1).addClass("active");
        });

        // Client-side validation for each step
        function validateStep(fieldset) {
            var isValid = true;
            fieldset.find("input[required], select[required], textarea[required]").each(function() {
                if (!$(this).val().trim()) {
                    isValid = false;
                    $(this).addClass("is-invalid");
                    Swal.fire({
                        icon: 'error',
                        title: 'Missing Field',
                        text: `Please fill in the ${$(this).attr('name')} field`,
                        confirmButtonColor: '#1b55e2'
                    });
                } else {
                    $(this).removeClass("is-invalid");
                }
            });
            return isValid;
        }

        // Form submission
        $('#form-wizard1').on('submit', function(e) {
            e.preventDefault();

            // Validate all fields before submission
            var allValid = true;
            $("fieldset").each(function() {
                if (!validateStep($(this))) {
                    allValid = false;
                }
            });

            if (!allValid) {
                return;
            }

            // Check file inputs
            if (!$('input[name="idcardFront"]').get(0).files.length || !$('input[name="idcardBack"]').get(0).files.length) {
                Swal.fire({
                    icon: 'error',
                    title: 'Missing Files',
                    text: 'Please upload both front and back ID card images',
                    confirmButtonColor: '#1b55e2'
                });
                return;
            }

            const formData = new FormData(this);

            $.ajax({
                url: $(this).attr('action'),
                type: 'POST',
                data: formData,
                processData: false,
                contentType: false,
                success: function(response) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Success',
                        text: response.message,
                        confirmButtonColor: '#1b55e2'
                    }).then(() => {
                        window.location.reload();
                    });
                },
                error: function(xhr) {
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: xhr.responseJSON.error || 'An error occurred',
                        confirmButtonColor: '#1b55e2'
                    });
                }
            });
        });
    });
</script>


